    "fetch-mock": "^9.3.1",
    "html-webpack-plugin": "^4.0.4",
    "jest": "^25.2.7",
    "jest-date-mock": "^1.0.8",
    "onchange": "^6.1.0",
    "prettier": "^2.0.4",
    "prettier-loader": "^3.3.0",
    "redux-mock-store": "^1.5.4",
    "storybook": "^5.3.18",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11",
    "webpack-dev-middleware": "^3.7.2",
    "webpack-hot-middleware": "^2.25.0"
